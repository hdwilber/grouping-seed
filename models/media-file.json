{
  "name": "MediaFile",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string"
    },
    "type": {
      "type": "string"
    },
    "format": {
      "type": "string"
    },
    "url": {
      "type": "string"
    },
    "targetName": {
      "type": "string"
    },
    "created": {
      "type": "date",
      "required": true
    },
    "updated": {
      "type": "date",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "mediable": {
      "type": "belongsTo",
      "polymorphic": {
        "foreignKey": "mediableId",
        "discriminator": "mediableType"
      }
    }
  },
  "acls": [
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
